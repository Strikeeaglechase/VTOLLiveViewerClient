{
  "name": "vtol-live-viewer-client",
  "author": "Strikeeaglechase",
  "description": "Headless Client / Replay Tool for VTGR files",
  "version": "1.1.1",
  "private": true,
  "type": "module",
  "main": "dist/desktopApplication/index.js",
  "repository": "https://github.com/Strikeeaglechase/VTOLLiveViewerClient",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "scripts": {
    "build": "tsc && rollup -c rollup.config.js && rollup -c webworker.rollup.config.js && rollup -c graph.rollup.config.js && rollup -c sensor.rollup.config.js",
    "watch": "rollup -c rollup.config.js -w",
    "watch-graph": "rollup -c graph.rollup.config.js -w",
    "watch-sensor": "rollup -c sensor.rollup.config.js -w",
    "electron": "electron dist/desktopApplication/index.js",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@petamoriken/float16": "^3.9.2",
    "buffer": "^6.0.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-window-state": "^5.0.3",
    "jszip": "^3.10.1",
    "node": "^16.16.0",
    "rollup-plugin-visualizer": "^6.0.1",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "rstatsjs": "^1.0.1",
    "stats.js": "^0.17.0",
    "strik-2d-renderer": "^1.0.4",
    "three": "^0.136.0",
    "three-css2drender": "^1.0.0",
    "three.meshline": "^1.4.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.6",
    "update-electron-app": "^3.1.1",
    "vite": "^5.2.11"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/jszip": "^3.4.0",
    "@types/node": "^18.19.115",
    "@types/stats.js": "^0.17.0",
    "@types/three": "^0.136.1",
    "electron": "^37.1.0"
  }
}
